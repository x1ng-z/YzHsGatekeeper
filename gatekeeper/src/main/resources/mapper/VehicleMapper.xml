<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hs.gatekeeper.service.mapper.VehicleMapper">
  <resultMap id="baseVehicleInfo" type="com.hs.gatekeeper.model.entity.VehicleInfo">
    <!--<id column="id" jdbcType="INTEGER" property="id" />-->
    <result column="vehicle" jdbcType="VARCHAR" property="vehicleno" />
    <result column="vcardid" jdbcType="VARCHAR" property="cardno" />
  </resultMap>

  <select id="findVehicleInfoByCNo" parameterType="java.lang.String" resultMap="baseVehicleInfo">
    select vehicle, vcardid from levm_cardmanage  where  fstatusflag='1'  and nvl(dr,0)=0 and  vcardid=#{cardno}
  </select>

  <select id="findVehicleInfoByVNo" parameterType="java.lang.String" resultMap="baseVehicleInfo">
    SELECT ICCARD vcardid,CARNUMBER vehicle FROM  HS_FIXEDCARDSET_H  where  APPROVESTATUS=-1  and nvl(dr,0)=0 and CARNUMBER=#{vechelno}
  </select>

</mapper>
